{
  "name": "@smartimpact-it/locations-map",
  "version": "1.3.7",
  "description": "A library to display locations on a map.",
  "main": "dist/umd/index.js",
  "es2015": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/umd/index.js"
    },
    "./dist/": "./dist/",
    "./src/": "./src/",
    "./utils/": "./utils/",
    "./esm/": "./dist/esm/",
    "./css/": "./dist/css/",
    "./umd/": "./dist/umd/",
    "./types/": "./dist/types/",
    "./main.css": {
      "style": "./dist/css/main.css",
      "import": "./dist/css/main.css",
      "require": "./dist/css/main.css"
    },
    "./GoogleMapsWrapper": {
      "import": "./dist/esm/map-providers/GoogleMapsWrapper.js"
    },
    "./GoogleMapsClusteredWrapper": {
      "import": "./dist/esm/map-providers/GoogleMapsClusteredWrapper.js"
    },
    "./LeafletMapWrapper": {
      "import": "./dist/esm/map-providers/LeafletMapWrapper.js"
    },
    "./LeafletMapClusteredWrapper": {
      "import": "./dist/esm/map-providers/LeafletMapClusteredWrapper.js"
    },
    "./GoogleMapsGeocoderProvider": {
      "import": "./dist/esm/search-providers/GoogleMapsGeocoderProvider.js"
    },
    "./NominatimProvider": {
      "import": "./dist/esm/search-providers/NominatimProvider.js"
    },
    "./Pagination": {
      "import": "./dist/esm/pagination-provider/Pagination.js"
    }
  },
  "directories": {
    "lib": "dist"
  },
  "files": [
    "dist/**/*",
    "src/**/*",
    "utils/**/*"
  ],
  "sideEffects": false,
  "authors": [
    {
      "name": "SmartImpact IT",
      "email": "dev@smartimpact.it",
      "url": "https://smartimpact.fr"
    },
    {
      "name": "Bogdan Barbu",
      "email": "bbarbu@smartimpact.fr"
    }
  ],
  "license": "GPL-3",
  "keywords": [
    "map",
    "stores",
    "locations",
    "google maps",
    "leaflet"
  ],
  "scripts": {
    "watch": "webpack --progress --watch --color --config config/webpack.config.js",
    "start": "cross-env NODE_ENV=development webpack serve --open --config config/webpack.config.js",
    "build": "npm run build:umd && npm run build:esm && npm run build:types",
    "build:vite": "vite build",
    "build:esm": "node tools/cleanup esm && tsc -p config/tsconfig.esm.json && babel ./dist/esm -d ./dist/esm --config-file ./config/babel.config.esm.js",
    "build:umd": "node tools/cleanup umd && webpack --config config/webpack.config.js",
    "build:types": "node tools/cleanup types && tsc -p config/tsconfig.types.json",
    "test": "jest --config jestconfig.json --passWithNoTests",
    "format": "prettier --write \"src/**/*.ts\" \"**/*.md\"",
    "format:check": "prettier --list-different \"src/**/**.{js,ts}\" \"**/*.md\"",
    "pretest": "npm run build",
    "lint": "tslint -p tsconfig.json",
    "preversion": "npm run lint",
    "prepublishOnly": "npm test && npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/siit-dev/locations-map.git"
  },
  "dependencies": {
    "@types/google.maps": "^3.52.5",
    "@types/leaflet": "^1.9.3",
    "@types/leaflet.markercluster": "^1.5.1",
    "@types/list.js": "^2.3.1",
    "@types/lodash": "^4.14.192",
    "@types/markerclustererplus": "^2.1.33"
  },
  "peerDependencies": {
    "@googlemaps/js-api-loader": "^1.15.1",
    "@googlemaps/markerclustererplus": "^1.2.10",
    "@tarekraafat/autocomplete.js": "^10.2.7",
    "jquery": "^3.6.4",
    "leaflet": "^1.9.3",
    "leaflet.markercluster": "^1.5.3",
    "list.js": "^2.3.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/helpers": "^7.21.0",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.20.7",
    "@babel/plugin-transform-react-jsx": "^7.21.0",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-typescript": "^7.21.4",
    "@babel/register": "^7.21.0",
    "@googlemaps/js-api-loader": "^1.15.1",
    "@googlemaps/markerclustererplus": "^1.2.10",
    "@tarekraafat/autocomplete.js": "^10.2.7",
    "@types/jest": "^29.4.0",
    "@types/jquery": "^3.5.16",
    "autoprefixer": "^10.4.14",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.2",
    "babel-plugin-lodash": "^3.3.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "html-webpack-tags-plugin": "^3.0.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "leaflet": "^1.9.3",
    "leaflet.markercluster": "^1.5.3",
    "list.js": "^2.3.1",
    "mini-css-extract-plugin": "^2.7.5",
    "postcss": "^8.4.21",
    "postcss-loader": "^7.2.4",
    "prettier": "^2.8.7",
    "sass": "^1.62.0",
    "sass-loader": "^13.2.2",
    "style-loader": "^3.3.2",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.0.4",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.13.2",
    "webpack-notifier": "^1.15.0"
  },
  "bugs": {
    "url": "https://github.com/siit-dev/locations-map/issues"
  },
  "homepage": "https://github.com/siit-dev/locations-map",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom"
  }
}
